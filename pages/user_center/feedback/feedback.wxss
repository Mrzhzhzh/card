/* pages/user_center/feedback/feedback.wxss */
.feed_box{
  font-size: 32rpx;
  color: #6e6e6e;
  line-height: 2;
  margin-top: 30rpx;
  padding: 0 30rpx;
}
.feed_content{
  width: 660rpx;
  border: 2rpx #bbb solid;
  padding: 10rpx;
  font-size: 32rpx;
  color: #101010;
}
.feed_content1{
  height: 280rpx;
  line-height: 1.5;
}
.feed_content2{
  height: 120rpx;
  line-height: 140rpx;
}
.submit{
  width: 680rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  display: block;
  margin: 100rpx auto 0;
  border-radius: 6rpx;
  background: #FA9D26;
  font-size: 30rpx;
  color: #fff;
}
